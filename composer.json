{
    "name": "michelmelo/telegram-logger-errors",
    "description": "Send logs to Telegram chat via Telegram bot",
    "license": "MIT",
    "autoload":
    {
        "psr-4":
        {
            "MichelMelo\\Logger\\": "src"
        }
    },
    "authors": [
    {
        "name": "Michel Melo",
        "email": "eu@michelmelo.pt",
        "role": "author",
        "homepage": "https://michelmelo.pt"
    }],
    "require":
    {
        "php": "^7.1.3",
        "monolog/monolog": "^1.23|^2.0",
        "irazasyed/telegram-bot-sdk": "3.2.0",
        "laravel/framework": "5.6.*|5.7.*|5.8.*|^6.0|^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra":
    {
        "laravel":
        {
            "providers": [
                "MichelMelo\\Logger\\TelegramLoggerServiceProvider"
            ]
        }
    }
}